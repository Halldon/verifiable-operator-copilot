{
  "version": "1.1.0",
  "wallet": {
    "network": "base",
    "chainId": 8453,
    "custodyMode": "agent-keystore+keychain-passphrase"
  },
  "sovereignty": {
    "mode": "local-agent-key",
    "strictMode": false,
    "contractControl": {
      "chainId": 8453,
      "rpcEnv": "BASE_RPC_URL",
      "rpcUrl": "",
      "authorityContract": "",
      "approvalFunction": "isActionApproved",
      "consumedFunction": "isActionConsumed",
      "notes": "In contract-controlled mode, treasury execution must be approved onchain and executed by contract-owned treasury/safe module."
    }
  },
  "execution": {
    "preferGasless": true,
    "gaslessRelay": {
      "name": "ETHGas/Open Gas",
      "relayUrl": "",
      "apiKeyEnv": "ETHGAS_API_KEY"
    },
    "fallbackOnchain": true
  },
  "limits": {
    "maxUsdPerDay": 25,
    "maxUsdPerTx": 5,
    "allowedRecipients": [
      "0x581E81C3dc9F558D29dCd9877A828Cd61D7AaAeD"
    ]
  },
  "autofund": {
    "enabled": true,
    "targetService": "eigenai-grant",
    "minCreditsThreshold": 200000,
    "topupUsd": 5,
    "checkIntervalSeconds": 1800
  }
}
